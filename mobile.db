CREATE TABLE `mobile` (
	`id` INT(100) NOT NULL AUTO_INCREMENT,
	`date` DATETIME NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp(),
	`p2p` INT(100) NULL DEFAULT NULL,
	`portin` INT(100) NULL DEFAULT NULL,
	`data` INT(100) NULL DEFAULT NULL,
	`business` INT(100) NULL DEFAULT NULL,
	`total` FLOAT NOT NULL DEFAULT 0,
	PRIMARY KEY (`id`)
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=3
;
